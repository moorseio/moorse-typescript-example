{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "outDir": "./dist",
    // "rootDir": "./src",
    "strict": true,
    "strictPropertyInitialization": false,
    // "baseUrl": "./src",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strictNullChecks":false,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "__tests__"
  ]
}
